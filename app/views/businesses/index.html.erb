
<h1>Businesses</h1>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @businesses.each do |business| %>
      <tr>
        <td><%=business.name%></td>
        <td><%=business.address%></td>
        <td><%=business.city%></td>
        <td><%= link_to 'Show', business %></td>
        <td><%= link_to 'Edit', edit_business_path(business) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Back', items_path %>

<br>

